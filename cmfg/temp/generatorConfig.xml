<!DOCTYPE generatorConfiguration PUBLIC
        "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN"
        "http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd">
<generatorConfiguration>
    <context id="simple" targetRuntime="MyBatis3">
        <jdbcConnection driverClass="com.mysql.cj.jdbc.Driver"
                        connectionURL="jdbc:mysql://localhost:3306/ry?useUnicode=true&amp;characterEncoding=utf8&amp;zeroDateTimeBehavior=convertToNull&amp;useSSL=true&amp;serverTimezone=GMT%2B8"
                        userId="root"
                        password="123456"
        />

        <javaModelGenerator targetPackage="com.ruoyi.cmfg.domain" targetProject="src/main/java"/>

        <sqlMapGenerator targetPackage="com.ruoyi.cmfg.mapper" targetProject="src/main/resources"/>

        <javaClientGenerator type="XMLMAPPER" targetPackage="com.ruoyi.cmfg.mapper" targetProject="src/main/java"/>

        <table tableName="user" />
        <table tableName="demander" />
        <table tableName="task" />
        <table tableName="provider" />
        <table tableName="services" />
        <table tableName="oprator" />
        <table tableName="resource" />
        <table tableName="project" />
    </context>
</generatorConfiguration>